{
  "name": "cleanfile",
  "displayName": "Clean File",
  "description": "Extension para aplicar los archivos basicos de basados en Arquitectura Limpia",
  "version": "0.0.3",
  "publisher":"CleanFile",
  "repository":"https://github.com/lacien18/clean-file-extension-vscode.git",
  "icon": "/icon.png",
  "keywords":["dart","flutter","clean","clean-file","file-clean","richar correa","clean file","clean file generate","generate"],
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clean-file.generate",
        "title": "Clean File Generate"
      },
      {
        "command": "clean-file.feature",
        "title": "Clean File Generate Feature"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.13.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
